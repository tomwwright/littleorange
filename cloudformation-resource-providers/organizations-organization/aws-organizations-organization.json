{
  "typeName": "AWS::Organizations::Organization",
  "description": "Resource schema for AWS Organizations Organization resource",
  "sourceUrl": "https://github.com/tomwwright/lilorange.git",
  "definitions": {},
  "properties": {
    "Arn": {
      "type": "string"
    },
    "Id": {
      "type": "string",
      "pattern": "^o-[a-z0-9]{10,32}$"
    },
    "MasterAccountArn": {
      "type": "string"
    },
    "MasterAccountId": {
      "type": "string",
      "pattern": "^[0-9]{12}$"
    },
    "MasterAccountEmail": {
      "type": "string"
    },
    "FeatureSet": {
      "description": "Specifies the feature set supported by the organization. Each feature set supports different levels of functionality.",
      "type": "string",
      "enum": [
        "ALL",
        "CONSOLIDATED_BILLING"
      ]
    }
  },
  "additionalProperties": false,
  "required": [],
  "readOnlyProperties": [
    "/properties/Id",
    "/properties/MasterAccountArn",
    "/properties/MasterAccountId",
    "/properties/MasterAccountEmail"
  ],
  "primaryIdentifier": [
    "/properties/Id"
  ],
  "additionalIdentifiers": [
    [
      "/properties/Arn"
    ]
  ],
  "handlers": {
    "create": {
      "permissions": [
        "initech:CreateReport"
      ]
    },
    "read": {
      "permissions": [
        "initech:DescribeReport"
      ]
    },
    "update": {
      "permissions": [
        "initech:UpdateReport"
      ]
    },
    "delete": {
      "permissions": [
        "initech:DeleteReport"
      ]
    },
    "list": {
      "permissions": [
        "initech:ListReports"
      ]
    }
  }
}