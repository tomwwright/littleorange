      "Properties": {
        "NetworkAclId": {
          "Ref": "VPCPublicNACL"
        },
        "CidrBlock": "0.0.0.0/0",
        "Egress": true,
        "Protocol": -1,
        "RuleAction": "allow",
        "RuleNumber": 100
      }
    },
    "VPCPublicNACLIngress100": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
